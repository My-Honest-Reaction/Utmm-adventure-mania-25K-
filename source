local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Kavo-Ui/main/Darkrai%20Ui", true))()

local Library = DarkraiX:Window("Darkrai X","","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Credit")

Tab1:Button("UI Library = Darkrai X",function()
    print("Use Darkrai X UI library :D")
end)


Tab1:Button("Contact me, Discord = originalstar_walker for bugs",function()
    print("DM originalstar_walker on discord for help/ bugs")
end)


Tab1:Button("Nobody Likes brazilnumerocinco :/",function()
    print("tell brazilnumerocinco to Go Away")
end)



Tab1 = Library:Tab("TP PLACES")

Tab1:Button("TP to shop",function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-161.962646484375, 14.998133659362793, 311.5132131113281)
end)

Tab1:Button("TP to Event Area (Could be a fight)",function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-97.06958770751953, 15.662306785583496, 196.35775756835938)
end)


Tab1 = Library:Tab("Equip Owned Knifes")

Tab1:Button("Equip Stick",function()
    local args = {
 [1] = game:GetService("Lighting"):WaitForChild("Weapons"):WaitForChild("Stick")
 }
 
 game:GetService("Lighting"):WaitForChild("UseWeapon"):FireServer(unpack(args))
end)


Tab1:Button("Equip Toy Knife",function()
    local args = {
