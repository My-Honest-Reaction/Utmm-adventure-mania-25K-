Tab1 = Library:Tab("Buy Food")


Tab1:Button("Buy Monster Candy (10 GOLD)",function()
    local args = {
    [1] = game:GetService("Lighting"):WaitForChild("Food"):WaitForChild("MonsterCandy"),
    [2] = "Food"
    }
    
    Game:GetService("Lighting"):WaitForChild("Buy"):FireServer(unpack(args))
end)

